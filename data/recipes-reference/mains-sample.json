      "ingredients": [
        { "id": "lettuce-cos", "name": "Cos lettuce leaves", "grams": 60, "notes": "2 large leaves" },
        { "id": "avocado", "name": "Avocado", "grams": 60, "notes": "sliced" },
        { "id": "chicken-breast", "name": "Rotisserie chicken breast", "grams": 90, "notes": "shredded" },
        { "id": "tomato", "name": "Tomato", "grams": 40, "notes": "chopped" },
        { "id": "turkey-bacon", "name": "Turkey bacon", "grams": 30, "notes": "2 slices, cooked" },
        { "id": "ranch-dressing", "name": "Ranch dressing", "grams": 30, "notes": "Primal Kitchen or similar clean brand" },
        { "id": "black-pepper", "name": "Black pepper", "grams": 0.5 },
        { "id": "cheese-cheddar", "name": "Cheddar cheese", "grams": 28, "notes": "grated" }
      ],
      
      "instructions": [
        "Lay lettuce leaves flat on a board.",
        "Layer avocado slices on each leaf.",
        "Add shredded chicken, then tomato and turkey bacon.",
        "Drizzle with ranch dressing.",
        "Season with pepper and sprinkle with cheese.",
        "Fold up sides of lettuce and eat like a wrap."
      ],
      
