      "phase": [1, 2, 3],
      "category": "poultry",
      "cuisine": "universal",
      "tags": ["low-carb", "high-protein", "quick", "light-dinner"],
      
      "batch_friendly": true,
      "batch_notes": "Chicken and bacon can be prepped ahead. Assemble wraps just before eating.",
