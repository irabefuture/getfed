      "base_servings": 1,
      "ingredients": [
        { "id": "collagen-powder", "name": "Collagen powder", "grams": 20, "notes": "2 scoops" },
        { "id": "mct-powder", "name": "MCT powder or coconut oil", "grams": 10, "notes": "1 scoop" },
        { "id": "spinach", "name": "Fresh spinach", "grams": 30, "notes": "roughly chopped" },
        { "id": "blueberries", "name": "Fresh blueberries", "grams": 75 },
        { "id": "chia-seeds", "name": "Chia seeds", "grams": 24 },
        { "id": "flax-seeds", "name": "Flax seeds", "grams": 14 },
