        { "id": "eggs", "name": "Eggs", "grams": 100, "notes": "2 large" },
        { "id": "salt", "name": "Salt", "grams": 1 },
        { "id": "black-pepper", "name": "Black pepper", "grams": 0.5 },
        { "id": "butter", "name": "Butter", "grams": 14 },
        { "id": "spinach", "name": "Fresh spinach leaves", "grams": 30 },
        { "id": "tomato", "name": "Fresh tomatoes", "grams": 75, "notes": "chopped" },
        { "id": "raspberries", "name": "Fresh raspberries", "grams": 60, "notes": "to serve", "optional": true }
      ],

      "instructions": [
        "Crack the eggs into a medium bowl. Add a pinch of salt and pepper, and whisk until blended.",
        "Melt the butter in a medium frypan over low heat.",
        "Pour in the eggs and cook until the edges firm up.",
        "Gently fold the eggs over, then cook for another half-minute, pushing and folding around the pan.",
        "Add the spinach and tomatoes, and stir to incorporate.",
        "Continue pushing and folding until barely set. They should look slightly soft on top.",
        "Serve with fresh raspberries alongside if desired."
