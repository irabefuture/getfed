        { "id": "cocoa-powder", "name": "Unsweetened cocoa powder", "grams": 8 },
        { "id": "instant-coffee", "name": "Instant coffee granules", "grams": 2 },
        { "id": "sweetener-swerve", "name": "Swerve powdered sugar", "grams": 3 },
        { "id": "cheese-sticks", "name": "Cheese sticks", "grams": 170, "notes": "6 pieces" }
      ],

      "instructions": [
        "In a small nonstick frypan, toast almonds over low heat, stirring every couple of minutes, until fragrant, about 3 minutes.",
        "Add olive oil and stir to coat. Remove from heat.",
